name: ips
title: International Patient Summary (IPS)
resource_path: <%= resource_path %>
description : International Patient Summary (IPS)
fhir_version: r4
default_test_set: ad_hoc_testing
test_sets:
  ad_hoc_testing:
    view: default
    tests:
      - name: IPS Operations
        run_all: true
        sequences:
        - IpsDocumentOperationSequence
        - IpsSummaryOperationSequence
      - name: IPS Profiles
        run_all: true
        sequences:<% sequences.each do |sequence| %>
        - <%= sequence.class_name %><% end %>
sequence_requirements:<% sequences.flat_map { |seq| seq.requirements }.uniq.map { |req| req.gsub(':', '') }.each do |requirement| %>
  <%= requirement %>:
    label: <%= requirement %><% end %>
